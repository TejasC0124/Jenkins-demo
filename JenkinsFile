pipeline {
    agent any
    stages {
        stage('Checkout') {
            steps {
                git 'https://github.com/TejasC0124/Jenkins-demo.git'
            }
        }
        stage('Compile') {
            steps {
                bat 'javac Program.java'
            }
        }
        stage('Run') {
            steps {
                bat 'java Program'
            }
        }
    }
    post {
        success {
            archiveArtifacts artifacts: '*.class'
            echo 'CI PIPELINE SUCCESS'
        }
        failure {
            echo 'CI PIPELINE FAILED'
        }
    }
}
